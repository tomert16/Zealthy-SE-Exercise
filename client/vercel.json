{
    "version": 2,
    "builds": [
        {
            "src": "*.js",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        { "source": "/(.*)", "destination": "/index.html" }
    ],
    "redirects": [
      {
        "source": "/legacy/:path*",
        "destination": "/new/:path*"
      }
    ]
  }