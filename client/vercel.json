{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "redirects": [
        {
          "source": "/legacy/:path*",
          "destination": "/new/:path*"
        }
      ]
}